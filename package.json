{
    "name": "angular-sockjs-im"
  , "version": "0.0.1"
  , "private": true
  , "dependencies": {
      "express"    : ">=3.0.0"
    , "jade"       : ">= 0.26.1"
    , "sockjs"     : ">= 0.3.5"
    , "angular-cl2": "0.1.0"
